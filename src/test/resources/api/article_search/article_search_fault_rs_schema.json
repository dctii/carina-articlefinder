{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "fault": {
      "type": "object",
      "properties": {
        "faultstring": {
          "type": "string"
        },
        "detail": {
          "type": "object",
          "properties": {
            "errorcode": {
              "type": "string"
            }
          },
          "required": [
            "errorcode"
          ]
        }
      },
      "required": [
        "faultstring",
        "detail"
      ]
    }
  },
  "required": [
    "fault"
  ]
}
